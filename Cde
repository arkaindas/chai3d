const dynamicFileGroups = document.querySelectorAll('.contactUsFileUpload');

if (dynamicFileGroups.length > 0) {
  dynamicFileGroups.forEach((group, index) => {
    const fileInput = group.querySelector('input[type="file"]');

    if (fileInput) {
      const fieldKey = `uploadImageDynamic_${fileInput.id || index}`;
      const file = fileInput.files[0];

      if (file) {
        formData.set(fieldKey, file);
      } else {
        formData.set(fieldKey, null);
      }
    }
  });
}
